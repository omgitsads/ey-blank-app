socket = l:TCP_NODELAY=1
socket = r:TCP_NODELAY=1
foreground=no
pid=/var/run/stunnel/stunnel.pid
debug=3

[haproxy-<%= @app.name %>]
cert = /etc/nginx/ssl/<%= @app.name %>.crt
key = /etc/nginx/ssl/<%= @app.name %>.key
accept=0.0.0.0:443
connect=127.0.0.1:8443
TIMEOUTclose = 0
sendproxy=yes
ciphers = HIGH:!ADH:!MD5
options = NO_SSLv2
